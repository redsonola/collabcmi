    "@testing-library/user-event": "^7.2.1",
    "@tonejs/midi": "^2.0.25",
    "@tsconfig/svelte": "^1.0.10",
    "@types/jest": "^26.0.20",
    "@types/mathjs": "^6.0.9",
    "@types/node": "^14.14.31",
    "abr-xcorr": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "compute-cosine-similarity": "^1.0.0",
    "compute-covariance": "^1.0.1",
    "compute-l2norm": "^1.1.0",
