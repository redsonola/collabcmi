    "typescript": "^4.2.3",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "mkcert": "mkcert -install && mkcert localhost",
    "start": "cross-env HTTPS=true SSL_CRT_FILE=localhost.pem craco start",
    "pre-build": "rm -rf build || true",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "storybook": "start-storybook --https --ssl-cert localhost.pem --ssl-key localhost-key.pem -p 6006 -s public",
    "build-storybook": "build-storybook -o build/storybook",
    "peer-server": "node ./scripts/devPeerServer"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/node-logger": "^6.1.11",
    "@storybook/svelte": "^6.1.11",
    "anysort": "^2.0.0",
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.3",
    "husky": "^4.3.6",
    "peer": "^0.5.3",
    "svelte": "^3.35.0",
    "svelte-check": "^1.2.3",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^4.6.9"
  },
  "husky": {
    "hooks": {
      "pre-push": "rm -rf build && npm run build && npm run build-storybook"
    }
  }
